{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "astana",
  "version": "0.1.0",
  "identifier": "com.perogeremmer.astana",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Astana - Manajemen Iuran Makam",
        "width": 1366,
        "height": 768,
        "minWidth": 1024,
        "minHeight": 600,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
